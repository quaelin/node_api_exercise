    "sql.js": "^1.8.0"
  },
  "devDependencies": {
    "eslint": "^8.32.0",
    "jest": "^29.4.1",
    "supertest": "^6.3.3"
  },
  "eslintConfig": {
    "env": {
      "es6": true,
      "node": true,
      "jest": true
    },
    "parserOptions": {
      "ecmaVersion": 8
